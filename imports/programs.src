    A.hack=function(mlib,third="$",metaxploit=null)
        DB_dir=A.remote_server.host_computer.File("/root/shadow_data/exploits")
        if not DB_dir then A.remote_server.host_computer.create_folder(DB_dir.path,"exploits")
